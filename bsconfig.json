{
  "name": "gatsby-starter-reason",
  "namespace": true,
  "bs-dependencies": ["reason-react", "gatsby-link-reason"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-super-errors"],
  "reason": { "react-jsx": 3 },
  "refmt": 3
}
